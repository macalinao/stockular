{
  "manifest_version": 2,

  "name": "Bloomberg Chrome Extension",
  "description": "This extension provides financial details behind stock tickers.",
  "version": "1.0",
  "content_scripts": [{
    "css": ["highlight.min.css", "jquery.qtip.min.css"],
    "js": ["highlight.min.js", "stocks.js", "jquery.min.js", "jquery.qtip.min.js"],
    "matches": ["http://*/*", "https://*/*"]
  }]
}

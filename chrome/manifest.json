{
  "manifest_version": 2,

  "name": "Bloomberg Chrome Extension",
  "description": "This extension provides financial details behind stock tickers.",
  "version": "1.0",
  "content_scripts": [{
    "css": ["jquery.qtip.min.css", "style.css"],
    "js": ["stocks.js", "jquery.min.js", "highlight.min.js", "highstock.js", "jquery.qtip.min.js", "ext.js"],
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_end"
  }]
}
